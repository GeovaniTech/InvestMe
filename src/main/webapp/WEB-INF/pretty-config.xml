  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<url-mapping id="login">
		<pattern value="/login" />
		<view-id value="/login.xhtml" />
	</url-mapping>
	
	<url-mapping id="register">
		<pattern value="/register" />
		<view-id value="/register.xhtml" />
